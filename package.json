{
    "name": "openjscad-react",
    "version": "0.0.1-canary-02",
    "description": "React.js component for the OpenJSCAD.org project",
    "repository": "https://github.com/aeksco/openjscad-react",
    "author": "Alexander Schwartzberg <aeksco@gmail.com>",
    "license": "MIT",
    "scripts": {
        "start": "start-storybook -p 6006",
        "storybook:build": "build-storybook",
        "prettify": "prettier --config ./.prettierrc.js 'src/**/*.ts*' --write",
        "build": "npm run clean && npm run compile",
        "clean": "rm -rf ./dist",
        "compile": "tsc -p tsconfig.build.json",
        "prepublishOnly": "npm run build"
    },
    "files": [
        "dist"
    ],
    "main": "./dist/src/index.js",
    "types": "./dist/src/index.d.ts",
    "keywords": [
        "react",
        "reactjs",
        "openjscad",
        "scad",
        "cad",
        "stl",
        "3d-printing",
        "javascript",
        "typescript"
    ],
    "dependencies": {
        "@jscad/csg": "^0.7.0",
        "@jscad/stl-serializer": "^0.2.0",
        "@jscad/web": "^1.10.0",
        "classnames": "^2.2.6"
    },
    "peerDependencies": {
        "react": ">=16",
        "react-dom": ">=16"
    },
    "devDependencies": {
        "@storybook/addon-docs": "6.0.26",
        "@storybook/addon-knobs": "6.0.26",
        "@storybook/addons": "6.0.26",
        "@storybook/react": "6.0.26",
        "@types/classnames": "^2.2.10",
        "@types/jest": "^26.0.14",
        "@types/lodash.debounce": "^4.0.6",
        "@types/react": "^16.9.51",
        "@types/react-dom": "^16.9.8",
        "copy-dir": "^1.3.0",
        "jest": "^26.5.2",
        "lodash.debounce": "^4.0.8",
        "postcss-preset-env": "^6.7.0",
        "prettier": "^2.1.2",
        "react": "^16.13.1",
        "react-docgen-typescript-loader": "^3.7.2",
        "react-dom": "^16.13.1",
        "tailwindcss": "^1.8.10",
        "terser-webpack-plugin": "^4.2.3",
        "ts-jest": "^26.4.1",
        "ts-loader": "^8.0.4",
        "typescript": "^3.9.7"
    }
}
